#, c-format
msgid "  -T LIST, --tests=LIST       List of tests to run\n"
msgstr "  -T LIST, --tests=LIST       동작하려는 시험 목록\n"

#: src/rpminspect.c:54
#, c-format
msgid "                                (default: ALL)\n"
msgstr "                                (기본값: ALL)\n"

#: src/rpminspect.c:55
#, c-format
msgid "  -E LIST, --exclude=LIST     List of tests to exclude\n"
msgstr "  -E LIST, --exclude=LIST     예외 시험 목록\n"

#: src/rpminspect.c:56
#, c-format
msgid "                                (default: none)\n"
msgstr "                                (기본값: none)\n"

