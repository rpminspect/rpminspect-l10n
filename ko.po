#: src/rpminspect.c:49
#, c-format
msgid "Usage: %s [OPTIONS] [before build] [after build]\n"
msgstr "사용법: %s [OPTIONS] [before build] [after build]\n"

#: src/rpminspect.c:50
#, c-format
msgid "Options:\n"
msgstr "옵션:\n"

#: src/rpminspect.c:51
#, c-format
msgid "  -c FILE, --config=FILE      Configuration file to use\n"
msgstr "  -c FILE, --config=FILE      사용하려는 구성파일\n"

